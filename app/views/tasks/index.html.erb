<h1>All your tasks</h1>


<% @tasks.each do |task| %>
  <h2><%= task.name %></h2>
  <p><%= task.description %></p>
  <%= link_to "See task", task_path(task) %>
  <%= link_to "Delete task", task_path(task), method: :delete, data: {confirm: "Are you sure?"} %>
<% end %>




rails g migration AddCoordinatesTo*Flats* latitude:float longitude:float
generate migration AddDoneToTasks Done:string
